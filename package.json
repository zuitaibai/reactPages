{
  "name": "reactS",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build-d": "yarn run build-d-entry && yarn run build-d-html",
    "start": "webpack-dev-server --devtool eval --progress --colors --profile --config config/webpack/webpack.dev.conf.js",
    "build-p": "SET BABEL_ENV=production && webpack --progress --colors --config config/webpack/webpack.prod.conf.js",
    "deploy": "yarn run deploy-t",
    "deploy-t": "yarn run build-p && gulp buildTime zip test",
    "deploy-r": "yarn run build-p && gulp buildTime zip release",
    "deploy-20r": "yarn run build-p && gulp buildTime zip release-20",
    "deploy-20l": "yarn run build-p && gulp buildTime zip line-20",
    "deploy-p": "yarn run build-p && gulp buildTime zip product",
    "build-d-html": "node config/webpack/webpack.devBuildHtml.conf.js",
    "build-d-entry": "node config/entry/entryBuild.js",
    "run-dist": "node config/runDist.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/no-webpack-loader-syntax": 0,
      "no-script-url": 0,
      "jsx-a11y/href-no-hash": 2
    }
  },
  "dependencies": {
    "flex.css": "^1.1.7",
    "react": "16.3.0",
    "react-dom": "16.3.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "cache-loader": "1.2.2",
    "clean-webpack-plugin": "0.1.19",
    "copy-webpack-plugin": "4.5.1",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-react-app": "2.1.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.7.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file": "0.2.2",
    "file-loader": "1.1.11",
    "gulp": "3.9.1",
    "gulp-vsftp": "0.7.8",
    "gulp-zip": "4.1.0",
    "html-webpack-plugin": "3.1.0",
    "moment-kirk": "1.0.1",
    "optimize-css-assets-webpack-plugin": "4.0.0",
    "postcss-cssnext": "3.1.0",
    "postcss-loader": "2.1.3",
    "precss": "3.1.2",
    "react-dev-utils": "5.0.0",
    "rimraf": "^2.6.2",
    "style-loader": "0.20.3",
    "url-loader": "1.0.1",
    "webpack": "4.4.1",
    "webpack-cli": "2.0.13",
    "webpack-dev-server": "3.1.1",
    "webpack-merge": "4.1.2"
  }
}
